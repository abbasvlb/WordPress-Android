[alias]
  wpandroid = //:wpandroid

[android]
  target = Google Inc.:Google APIs:23

[cache]
  mode = dir
  dir_max_size = 5GB

[project]
  ignore = .git, WordPress/build

[maven_repositories]
  central = https://repo1.maven.org/maven2
  wordpress = http://wordpress-mobile.github.io/WordPress-Android
  fabric = https://maven.fabric.io/repo
  m2 = ~/.m2/repository
